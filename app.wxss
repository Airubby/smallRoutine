/**app.wxss**/
view text{
  margin: 0;
  padding: 0;
}
.index_top {
    width: 100%;
    height: 45px;
    padding: 10px 0;
    background: #11A9EC;
}
.index_logo{
  float: left;
  margin-left: 10px;
}
.index_logo image{
  width: 100px;
  height: 45px;
}
.index_phone{
  float: right;
  height: 45px;
  font-size: 12px;
  margin-right: 10px;
  color: #fff;
  line-height: 70px;
}
.slide-image{
  width: 100%;
}
.content{
  background: #F2F2F2;
  padding: 0 10px;
  overflow: hidden;
}
.footer{
  width: 100%;
  height: 45px;
  line-height: 45px;
  font-size: 14px;
  text-align: center;
  background: #1A89B9;
  color:#fff;
}
.mb20{
  margin-bottom: 20px;
}
.fl{
  float: left;
}
.panel-body .media-heading{
    margin-bottom: 5px;
    display: block;
    overflow: hidden;
    font-size: 14px;
}
.panel-body .media-body {
    font-size: 12px;
    color: #666;
    display: block;
}
.panel-body .panel-body-li{
    padding-bottom: 15px;
    border-bottom: 1px solid #e2e2e2;
    margin-bottom: 10px;
}
.panel-body .panel-body-li:last-of-type{
  border: none;
  padding-bottom: 0;
  margin-bottom: 0;
}